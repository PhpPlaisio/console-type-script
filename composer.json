{
  "name": "plaisio/console-type-script",
  "description": "PhpPlaisio: Console TypeScript",
  "keywords": [
    "PhpPlaisio",
    "Console",
    "TypeScript"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-dom": "*",
    "ext-inotify": "*",
    "ext-libxml": "*",
    "plaisio/console": "^3.0.0",
    "plaisio/console-assets": "^1.1.0",
    "plaisio/kernel": "^3.2.0",
    "setbased/exception": "^2.3.0",
    "setbased/helper-program-execution": "^1.2.0",
    "setbased/typed-config": "^2.0.0",
    "symfony/console": "^6.2.5",
    "symfony/filesystem": "^6.2.5"
  },
  "require-dev": {
    "phing/phing": "^3.0.0",
    "phpunit/phpunit": "^10.5.35"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Console\\TypeScript\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Console\\TypeScript\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
